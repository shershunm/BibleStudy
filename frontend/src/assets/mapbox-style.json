{
  "version": 8,
  "name": "Dark-Mode-Precision",
  "metadata": {
    "mapbox:autocomposite": true,
    "mapbox:type": "template"
  },
  "sources": {
    "mapbox": {
      "type": "vector",
      "url": "mapbox://mapbox.mapbox-streets-v8"
    }
  },
  "glyphs": "mapbox://fonts/mapbox/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#000000"
      }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "mapbox",
      "source-layer": "water",
      "paint": {
        "fill-color": "#0033FF",
        "fill-opacity": 0.6
      }
    },
    {
      "id": "land",
      "type": "background",
      "paint": {
        "background-color": "#000000"
      }
    },
    {
      "id": "landcover",
      "type": "fill",
      "source": "mapbox",
      "source-layer": "landcover",
      "paint": {
        "fill-color": "#0a0a0a",
        "fill-opacity": 0.3
      }
    },
    {
      "id": "national-park",
      "type": "fill",
      "source": "mapbox",
      "source-layer": "landuse_overlay",
      "filter": ["==", "class", "national_park"],
      "paint": {
        "fill-color": "#1a1a1a",
        "fill-opacity": 0.5
      }
    },
    {
      "id": "landuse",
      "type": "fill",
      "source": "mapbox",
      "source-layer": "landuse",
      "paint": {
        "fill-color": "#0a0a0a",
        "fill-opacity": 0.2
      }
    },
    {
      "id": "waterway",
      "type": "line",
      "source": "mapbox",
      "source-layer": "waterway",
      "paint": {
        "line-color": "#0033FF",
        "line-width": 1,
        "line-opacity": 0.8
      }
    },
    {
      "id": "building",
      "type": "fill",
      "source": "mapbox",
      "source-layer": "building",
      "paint": {
        "fill-color": "#1a1a1a",
        "fill-opacity": 0.6
      }
    },
    {
      "id": "tunnel-street-minor",
      "type": "line",
      "source": "mapbox",
      "source-layer": "road",
      "filter": ["all", ["==", "structure", "tunnel"], ["in", "class", "street", "street_limited"]],
      "paint": {
        "line-color": "#2a2a2a",
        "line-width": 1,
        "line-dasharray": [3, 3]
      }
    },
    {
      "id": "road-street",
      "type": "line",
      "source": "mapbox",
      "source-layer": "road",
      "filter": ["in", "class", "street", "street_limited"],
      "paint": {
        "line-color": "#333333",
        "line-width": {
          "base": 1.5,
          "stops": [[12, 0.5], [14, 2], [18, 18]]
        }
      }
    },
    {
      "id": "road-primary",
      "type": "line",
      "source": "mapbox",
      "source-layer": "road",
      "filter": ["==", "class", "primary"],
      "paint": {
        "line-color": "#444444",
        "line-width": {
          "base": 1.5,
          "stops": [[5, 0.75], [18, 32]]
        }
      }
    },
    {
      "id": "road-motorway",
      "type": "line",
      "source": "mapbox",
      "source-layer": "road",
      "filter": ["==", "class", "motorway"],
      "paint": {
        "line-color": "#555555",
        "line-width": {
          "base": 1.5,
          "stops": [[5, 0.75], [18, 32]]
        }
      }
    },
    {
      "id": "admin-country",
      "type": "line",
      "source": "mapbox",
      "source-layer": "admin",
      "filter": ["all", ["==", "admin_level", 0], ["==", "maritime", 0]],
      "paint": {
        "line-color": "#FFFFFF",
        "line-opacity": 0.3,
        "line-width": 2
      }
    },
    {
      "id": "admin-state",
      "type": "line",
      "source": "mapbox",
      "source-layer": "admin",
      "filter": ["all", ["==", "admin_level", 1], ["==", "maritime", 0]],
      "paint": {
        "line-color": "#FFFFFF",
        "line-opacity": 0.2,
        "line-width": 1,
        "line-dasharray": [2, 2]
      }
    },
    {
      "id": "place-city",
      "type": "symbol",
      "source": "mapbox",
      "source-layer": "place_label",
      "filter": ["==", "type", "city"],
      "layout": {
        "text-field": "{name}",
        "text-font": ["Open Sans Bold", "Arial Unicode MS Bold"],
        "text-size": {
          "base": 1.2,
          "stops": [[7, 14], [11, 24]]
        }
      },
      "paint": {
        "text-color": "#FFFFFF",
        "text-halo-color": "#000000",
        "text-halo-width": 1.5
      }
    },
    {
      "id": "place-town",
      "type": "symbol",
      "source": "mapbox",
      "source-layer": "place_label",
      "filter": ["==", "type", "town"],
      "layout": {
        "text-field": "{name}",
        "text-font": ["Open Sans Regular", "Arial Unicode MS Regular"],
        "text-size": {
          "base": 1.2,
          "stops": [[10, 12], [15, 18]]
        }
      },
      "paint": {
        "text-color": "#FFFFFF",
        "text-halo-color": "#000000",
        "text-halo-width": 1
      }
    },
    {
      "id": "road-label",
      "type": "symbol",
      "source": "mapbox",
      "source-layer": "road_label",
      "layout": {
        "text-field": "{name}",
        "text-font": ["Open Sans Regular", "Arial Unicode MS Regular"],
        "text-size": 10,
        "symbol-placement": "line"
      },
      "paint": {
        "text-color": "#FFFFFF",
        "text-halo-color": "#000000",
        "text-halo-width": 1
      }
    },
    {
      "id": "water-label",
      "type": "symbol",
      "source": "mapbox",
      "source-layer": "water_label",
      "layout": {
        "text-field": "{name}",
        "text-font": ["Open Sans Italic", "Arial Unicode MS Regular"],
        "text-size": 12
      },
      "paint": {
        "text-color": "#FFFFFF",
        "text-halo-color": "#0033FF",
        "text-halo-width": 1
      }
    }
  ]
}
